{
  "name": "vite-plugin-svgr",
  "version": "2.2.0-patch.1",
  "description": "Vite plugin to transform SVGs into React components",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": "./client.d.ts"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && tsc --module esnext --outDir temp && mv temp/index.js dist/index.mjs && rm -rf temp",
    "prepack": "rm -rf lib && npm run build"
  },
  "repository": "pd4d10/vite-plugin-svgr",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "keywords": [
    "vite",
    "vite-plugin"
  ],
  "author": "Rongjian Zhang",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "typescript": "^4.7.4",
    "vite": "^2.9.12"
  },
  "peerDependencies": {
    "@types/react": "^16.8 || 17.x || 18.x",
    "vite": "^2.6.0 || 3.x"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "@svgr/core": "^6.2.1"
  },
  "packageManager": "yarn@3.2.1"
}
